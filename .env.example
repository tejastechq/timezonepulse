# Example environment variables - copy to .env.local for development
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
NEXT_SHARP_PATH=./node_modules/sharp
NEXT_RSC_STRICT_MODE=false

# React 19 + Next.js 15 optimizations
NEXT_WEBPACK_USEPOLLING=false
NEXT_IGNORE_INCORRECT_LOCKFILE=true
NEXT_PREFER_OFFLINE=true

# Analytics configuration
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Add your Sentry DSN here
NEXT_PUBLIC_SENTRY_DSN=

# Add any other sensitive environment variables here